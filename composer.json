{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "danielh-official/ynab-sdk-laravel": "^1.0.4",
        "fakerphp/faker": "^1.24.1",
        "guzzlehttp/guzzle": "^7.9.2",
        "laravel/forge-cli": "^1.8.3",
        "laravel/framework": "^11.35.0",
        "laravel/sanctum": "^4.0.6",
        "laravel/tinker": "^2.10",
        "laravel/vapor-cli": "^1.65.2",
        "laravel/vapor-core": "^2.37.8",
        "maatwebsite/excel": "^3.1.61",
        "sentry/sentry-laravel": ">=4.10.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.2.2",
        "laravel/pint": "^1.18.3",
        "laravel/sail": "^1.39.1",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.5",
        "pestphp/pest": "^3.7",
        "pestphp/pest-plugin-laravel": "^3.0",
        "projektgopher/whisky": "^0.7.0",
        "spatie/laravel-ignition": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": [
            "@php artisan test --parallel"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
